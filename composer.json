{
    "name": "flammel/zweig",
    "description": "Twig extension for component-based templating.",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": ">=7.3.0",
        "twig/twig": "^3.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^0.11.19",
        "phpunit/phpunit": "^8.0",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "Flammel\\Zweig\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Flammel\\Zweig\\Tests\\": "tests/"
        }
    }
}
